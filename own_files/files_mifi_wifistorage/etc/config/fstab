
config global
        option anon_swap '0'
        option anon_mount '0'
        option auto_swap '1'
        option auto_mount '1'
        option delay_root '5'
        option check_fs '0'

config mount
        option target '/mnt/mmcblk0p1'
        option enabled '1'
        option device '/dev/mmcblk0p1'
        option fstype 'vfat'
        option options 'rw,sync,umask=000'
