config wifidog 'wifidog'
	option gateway_id '000000000001'
	option gateway_hostname 'cloud.xintenda.com'
	option gateway_httpport '80'
	option gateway_path '/'
	option gateway_connmax '50'
	option wifidog_enable '0'
	option enable '1'
	option gatewayport '2060'
	option check_interval '60'
	option client_timeout '1440'
	option ssl_enable	'no'
	option sslport		'443'

